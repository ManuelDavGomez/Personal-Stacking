        "cd my-koa-app",
        "npm init -y",
        "npm install koa koa-router mongoose dotenv",
        "npm install -D nodemon"
      ]
    },
    {
      "name": "NTP",
      "description": "NestJS con TypeORM y PostgreSQL",
