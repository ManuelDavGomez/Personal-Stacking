      ],
      "commands": [
        "python -m venv venv",
        "venv\\Scripts\\activate",
        "pip install flask sqlalchemy psycopg2-binary",
        "mkdir myflask",
        "cd myflask",
        "echo.>app.py"
      ]
    },
    {
      "name": "RP",
