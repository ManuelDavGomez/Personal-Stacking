        "npm create vite@latest my-vue-app -- --template vue",
        "cd my-vue-app",
        "npm install",
        "npm install vuex@next",
        "npm install -D tailwindcss postcss autoprefixer",
        "npx tailwindcss init -p"
      ]
    },
    {
      "name": "NTP",
