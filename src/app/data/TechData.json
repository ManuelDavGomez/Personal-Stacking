      ],
      "commands": [
        "npm create vite@latest my-solid-app -- --template solid",
        "cd my-solid-app",
        "npm install"
      ]
    },
    {
      "name": "RPP",
      "description": "Remix con PostgreSQL y Prisma ORM",
